<div id="home" class="animated bounce">
	<h1>{{title}}</h1>
</div>

<div id="app">
	<div id="chat">
		<ul id="messages">
			<li v-for="m in messages">
			  ${m.date | moment} ${ m.text }
			</li>
		</ul>
		
		<form action="">
		  <input id="m" autocomplete="off" placeholder="Say a little prayer for Zoubab"/><button>Send</button>
		</form>
		<script src="https://cdn.socket.io/socket.io-1.2.0.js"></script>
		<script src="http://code.jquery.com/jquery-1.11.1.js"></script>
	</div>
</div>